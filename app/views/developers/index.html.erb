<% @developers.each do |developer| %>
  <p>
    <%= developer.first_name %> <%= developer.last_name %>
  </p>
<% end %>

<div class="container">
  <div class="list">
    <div class="cards">
      <div class="container_card">
        <%= link_to "#", class: "thumbnail" do %>
          <%= image_tag "https://images.unsplash.com/photo-1438761681033-6461ffad8d80?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=750&q=80" %>
        <% end %>

        <div class="description">
          <div class="full_name">
            <h2><%= "toto last name" %></h2>
            <h3><%= "dev.last_name" %></h3>
          </div>
          <div class="experiences">
            <h2><%= "deux ans d'exp" %></h2>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div id="map">
  <img src="" alt="">
</div>
