<div class="navbar">
  <div class="logo">
    <%= link_to image_tag("logo_nav.png"), root_path%>
  </div>
  <div class="compteUser">
    <% if user_signed_in? %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class:"nav_bouton" %>
    <% else %>
         <%=  link_to "Sign in", new_user_session_path, class:"nav_bouton"%>
    <% end %>
     <% if user_signed_in? %>
          <%=link_to "Mon compte", user_path(current_user), class:"nav_bouton" %>
       <% else %>
          <%=link_to "Mon compte", new_user_session_path, class:"nav_bouton" %>
       <% end %>
    <%= link_to "#", class: "thumbnail" do %>
      <%= image_tag "https://images.unsplash.com/photo-1438761681033-6461ffad8d80?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=750&q=80" %>
    <% end %>
  </div>
</div>
